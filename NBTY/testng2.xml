<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="SmokeTest" xmlns="http://testng.org/testng-1.0.dtd">


    <test name="TestSet_Search" enabled="true">
        <groups>
            <run>
                <include name="searchtest"/>
            </run>
        </groups>

        <classes>
            <class name="tests.SearchTest"/>
        </classes>
    </test>

    <test name="TestSet_Login" enabled="true">
        <groups>
            <run>
                <include name="logintest"/>
            </run>
        </groups>

        <classes>
            <class name="tests.LoginTest"/>
        </classes>
    </test>

